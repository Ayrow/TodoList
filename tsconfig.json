{
  "compileOnSave": true,
  "compilerOptions": {
    "outDir": "./built",
    "allowJs": true,
    "target": "es5",
    "jsx": "react-jsx",
    "allowUnreachableCode": false,
    "noImplicitReturns": true,
    "noImplicitAny": true,
    "typeRoots": ["./typings"]
    // "outFile": "./built/combined.js"
  },
  "include": ["./**/*"],
  "exclude": ["./plugins/**/*", "./typings/**/*", "./built/**/*"]
}
